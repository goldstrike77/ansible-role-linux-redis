REDIS_EXPORTER_OPTS='-web.listen-address="{{ ansible_default_ipv4.address }}:{{ redis_exporter_port }}" -redis.addr="{% if redis_authorization | bool and redis_ssl | bool and redis_version.split('.')[0] | int > 5 %}rediss{% else %}redis{% endif %}://{{ ansible_default_ipv4.address }}:{{ redis_port }}" -skip-tls-verification -log-format="logger:syslog?appname=redis_exporter&local=7" -redis.password="{{ redis_requirepass | default("changeme") }}"'
